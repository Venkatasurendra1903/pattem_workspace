<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test name="TestGoogle" enabled="true">
		<classes>
			<class name="com.suri.test.EnableAndDesable">
				<methods>
					<include name="TestGoogle"/>
				</methods>
			</class>
		</classes>
	</test>
	<test name="facebook">
	<parameter name="browserName" value="edge"/>
		<classes>
			<class name="com.suri.test.EnableAndDesable">
				<methods>
					<include name="facebook"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>